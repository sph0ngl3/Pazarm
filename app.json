{
  "expo": {
    "name": "PazarM",
    "slug": "pazarm-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "https://i.ibb.co/78tp1Jw/icon.png",
    "scheme": "pazarm",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "https://i.ibb.co/78tp1Jw/icon.png"
    },
    "splash": {
      "image": "https://i.ibb.co/d0JPqzR6/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    }
  }
}
